  function moveChristmasball(el) {
    var style = el.style, height = browserHeight, radius, top;
    radius = parseInt(style.border, 10);
    top = parseInt(style.top, 10);
    top += Math.cos(moveAngle) + 1 + radius/2;
    if (top > height) {
      resetChristmasball(el);
    } else {
